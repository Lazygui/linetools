{
    "name": "linetools",
    "version": "1.0.0",
    "description": "命令行工具：目前仅支持HTTP/HTTPS请求",
    "main": "linetools.js",
    "bin": {
        "linetools": "linetools.js"
    },
    "scripts": {
        "dev": "node linetools.js",
        "build": "pkg ."
    },
    "dependencies": {
        "@inquirer/prompts": "^7.8.0",
        "alphabetjs": "^0.1.4",
        "chalk": "^4.1.2",
        "os-locale": "^6.0.2",
        "sqlite3": "^5.1.7",
        "string-width": "^7.2.0",
        "table": "^6.9.0"
    },
    "devDependencies": {
        "pkg": "^5.8.1"
    },
    "pkg": {
        "targets": [
            "node18-win-x64"
        ],
        "outputPath": "./",
        "assets": [
            "./modules/**/*",
            "./node_modules/sqlite3/build/Release/node_sqlite3.node"
        ]
    }
}